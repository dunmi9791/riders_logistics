<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

    <record id="increment_orderno_sequence" model="ir.sequence">
        <field name="name">Order Incrementation</field>
        <field name="code">increment_orderno</field>
        <field name="prefix">Order/%(month)s/%(range_year)s/</field>
        <field eval="True" name="use_date_range"/>
        <field name="padding">6</field>
        <field name="company_id" eval="False"/>
    </record>
        <record id="increment_collection_sequence" model="ir.sequence">
        <field name="name">Collection Incrementation</field>
        <field name="code">increment_collection</field>
        <field name="prefix">Collection No. /%(month)s/%(range_year)s/</field>
        <field eval="True" name="use_date_range"/>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    </data>
</odoo>